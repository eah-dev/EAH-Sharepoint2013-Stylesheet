    "jquery": "^3.3.1",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "require-dir": "^1.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.2.0"
  }
