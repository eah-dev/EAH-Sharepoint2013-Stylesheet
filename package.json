    "css-minify": "npm-run-all --parallel css-minify-style css-minify-docs",
    "css-minify-style": "gulp css-minify-style",
    "css-minify-docs": "gulp css-minify-docs",
    "docs": "npm-run-all --sequential docs-prebuild docs-build",
    "docs-prebuild": "gulp build-docs",
    "docs-build": "bundle exec jekyll build",
    "docs-lint": "gulp css-lint-docs"
  },
