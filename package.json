    "gulp-sourcemaps": "^2.6.2",
    "gulp-spsave": "^3.1.0",
    "gulp-stylelint": "^4.0.0",
    "npm-run-all": "^4.1.2",
    "require-dir": "^0.3.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^1.5.2"
  }
